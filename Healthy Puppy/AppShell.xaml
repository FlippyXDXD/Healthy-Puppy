<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
       xmlns:local="clr-namespace:Healthy_Puppy"
       x:Class="Healthy_Puppy.AppShell">

    <ShellContent Title="Página Principal" ContentTemplate="{DataTemplate local:MainPage}" />
    <ShellContent Title="Registro" ContentTemplate="{DataTemplate Vistas:RegistrationPage}" />
    <ShellContent Title="Lista de Dueños" ContentTemplate="{DataTemplate Vistas:OwnersListPage}" />
    <ShellContent Title="Control Médico" ContentTemplate="{DataTemplate Vistas:ControlMedicoPage}" />
    <ShellContent Title="Diagnóstico" ContentTemplate="{DataTemplate Vistas:DiagnosticoPage}" />
    <ShellContent Title="Configuraciones" ContentTemplate="{DataTemplate Vistas:ConfiguracionPage}" />

</Shell>
